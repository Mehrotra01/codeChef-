class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc= new Scanner(System.in);
		int t = sc.nextInt();
		for(int i=0;i<t;i++){
		    int x1 =sc.nextInt();
		    int y1 =sc.nextInt();
		    int x2 =sc.nextInt();
		    int y2 =sc.nextInt();
		    if( ( (x1-x2)%2==0 && (y1-y2)%2==0) || ((x1-x2)%2!=0 && (y1-y2)%2!=0) ){
		        System.out.println("YES");
		    } else{
		        System.out.println("NO");
		    }
		}
	}
}
